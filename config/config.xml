<?xml version="1.0" encoding="UTF-8"?>
<Installer>
  <Name>Haskell</Name>
  <Version>1.0.0</Version>
  <Title>Haskell</Title>
  <Publisher>Haskell.org</Publisher>
  <ProductUrl>https://www.haskell.org</ProductUrl>
  <StartMenuDir>Haskell</StartMenuDir>
  <TargetDir>@HomeDir@/Haskell</TargetDir>
  <AllowNonAsciiCharacters>true</AllowNonAsciiCharacters>
  <RemoteRepositories>
    <Repository>
      <DisplayName>Haskell main online repository</DisplayName>
      <Url>http://localhost:8080/repositories/main</Url>
    </Repository>
  </RemoteRepositories>
  <RepositoryCategories>
    <RemoteRepositories>
      <DisplayName>Latest releases</DisplayName>
      <Tooltip>Latest stable releases</Tooltip>
      <Preselected>true</Preselected>
    </RemoteRepositories>
    <RemoteRepositories>
      <DisplayName>Prereleases</DisplayName>
      <Tooltip>Latest prereleases, including alpha, beta and RC releases</Tooltip>
      <Preselected>true</Preselected>
    </RemoteRepositories>
    <RemoteRepositories>
      <DisplayName>Older releases</DisplayName>
      <Tooltip>Older stable releases</Tooltip>
    </RemoteRepositories>
  </RepositoryCategories>
</Installer>
